page{
  height: 100%;
}
.list-tab{
  height: 80rpx;
  padding: 5rpx;
}
.list-tab ul{
  display: flex;
  /* height: 72rpx; */
  height: 70%;
}
.list-tab li{
  flex: 1;
  display: flex;
  font-size: 11pt;
  justify-content: center;
  align-items: center;
  color: #888888;
}
.list-tab .active{
  color: #13D1BE;
}
.page-nodata{
  width: 100%;
  height: 100%;
  text-align: center;
  vertical-align: middle;
  color: #888888;
}
/*list-box*/
.list-view{
  border-top: 2rpx solid #EEE;
}
.list-box{
  width: 100%;
  height: 120rpx;
  display: flex;
  padding: 10rpx 30rpx;
  border-bottom: 2rpx solid #EEE;
}
 .list-images{
  width: 120rpx;
  height: 120rpx;
  vertical-align: middle;
  text-align: center;
} 
.list-images image{
  margin-top: 20rpx;
  width: 78rpx;
  height: 78rpx;
}
.list-text {
  width: 550rpx;
  height: 110rpx;
  margin-left: 20rpx;
  padding-top: 10rpx;
  color: #353535;
  /* background-size:60rpx; */
}
.list-text .name{
  height: 50rpx;
  width: 550rpx;
  padding: 10rpx;
  font-size: 12pt;
}
.list-text .txt{
  font-size: 8pt;
  width: 100%;
  height: 30rpx;
  color:#b2b2b2;
  float: right;
  text-align: right;
}
.swiper-box{
  height: 100%;
}
.scroll-box{
  height: 100%;
}
.btn{
  position: fixed;
  top: 90%;
  left: 85%;
  width: 110rpx;
  height: 110rpx;
}
.btn image{
  width: 100rpx;
  height: 100rpx;
}